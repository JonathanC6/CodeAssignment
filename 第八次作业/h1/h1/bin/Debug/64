    <script>
    // $(".navbar-ex1-collapse").on('click', function(event) {
    //   event.preventDefault();
    //   event.stopPropagation();
    //   $(this).collapse('hide');
    // });
    $(".toggleCode").each(function(){
      var code = $(this).find('.codeSpan');
      $(this).hover(function() {
        code.show();
      }, function() {
        code.hide();
      });
    })
    $('.navbar-nav > li.dropdown').hover(function (event) {
        event.preventDefault();
        $('ul.dropdown-menu', this).stop(true, true).slideDown('fast');
        $(this).addClass('open');
        event.stopPropagation();
    }, function (event) {
        event.preventDefault();
        $('ul.dropdown-menu', this).stop(true, true).slideUp('fast');
        $(this).removeClass('open');
        event.stopPropagation();
    });
    </script>

            <script>
    var sliderSource = [
          {
        img:"images/2024052101.jpg",
        txt:"“珞珈三号02星”卫星成功发射",
        url:"http://news.whu.edu.cn/info/1002/459617.htm"
      },
      {
        img:"images/2024052201.jpg",
        txt:"武汉大学女子排球队夺冠",
        url:"http://news.whu.edu.cn/info/1007/459687.htm"
      },
      {
        img:"images/2024050801.jpg",
        txt:"高考倒计时 | 相约珞珈山",
        url:"#"
      },
      {
        img:"images/2024042901.jpg",
        txt:"北京市丰台区人民政府、武汉大学签署《共建北京市丰台珞珈创新研究院合作协议》",
        url:"http://news.whu.edu.cn/info/1002/457337.htm"
      },
      {
        img:"images/2024042802.jpg",
        txt:"武汉大学第九届民族文化节举行",
        url:"http://news.whu.edu.cn/info/1007/457237.htm"
      }
    ];

    var slider = function(options,d){
      var defaults = {
        elmt:"",
        pause:6000,
        speed:1500
      }
      var settings = $.extend(true,{},defaults,options);
      var timer,_this=this;
      init();
      $(window).resize(function(event) {
        clearInterval(timer);
        cacheDom();
        _this.$wrap.css({'margin-left': 0});
        render(d);
        setSlide();
      });
      function init(){
        cacheDom();
        render(d);
        setSlide();
        bindEvt();
      }
      function cacheDom(){
        _this.$slider = $('#'+settings.elmt),
        _this.$indicator = _this.$slider.find('.carousel-indicators'),
        _this.$wrap = _this.$slider.find('.carousel-inner'),
        _this.$item = _this.$wrap.children('.item'),
        //_this.$width = _this.$slider.width(),
        _this.$width = $('.container').width();
        _this.$page = 1;
      }
      function render(d){
        _this.$wrap.empty();
        _this.$indicator.empty();
        for (var i = 0; i < d.length; i++) {
          var html = '<div class="item">'
                      +'  <img src="'+d[i].img+'" alt="...">'
                      +'  <div class="carousel-caption">'
                      +'    <a href="'+d[i].url+'">'+d[i].txt+'</a>'
                      +'  </div>'
                      +'</div>';
          _this.$wrap.append(html);
          var li = '<li></li>';
          _this.$indicator.append(li);
        };
        var fhtml = '<div class="item">'
                    +'  <img src="'+d[0].img+'" alt="...">'
                    +'  <div class="carousel-caption">'
                    +'    <a href="'+d[0].url+'">'+d[0].txt+'</a>'
                    +'  </div>'
                    +'</div>';
        _this.$wrap.append(fhtml);
        _this.$wrap.css('width', _this.$wrap.width()*_this.$wrap.children().length);
        _this.$wrap.children('.item').css('width', _this.$width);
        _this.$wrap.children('.item').eq(0).show();
        _this.$indicator.children().eq(0).addClass('active');
      }
      function bindEvt(){
        // _this.$wrap.hover(function() {
        //   clearInterval(timer);
        // }, function() {
        //   setSlide();
        // });
        _this.$indicator.children().on('click', function(event) {
          event.preventDefault();
          var i = $(this).index();
          if (!_this.$wrap.is(':animated')) {
            moveTo(i+1);
          };
        });
      }
      function setSlide(){
        timer = setInterval(function(){
          if (_this.$page<d.length) {
            _this.$wrap.animate({'margin-left': -_this.$width*_this.$page}, settings.speed);
            _this.$page++;
          }else{
            _this.$wrap.animate({
              'margin-left': -_this.$width*d.length
              },
              settings.speed, function() {
                _this.$wrap.css({'margin-left': 0});
            });
            _this.$page=1;
          };
          _this.$indicator.children().removeClass('active').eq(_this.$page-1).addClass('active');
        // console.log(_this.$page);
        }, settings.pause);
      }
      function moveTo(idx){
          clearInterval(timer);
          _this.$indicator.children().removeClass('active').eq(idx-1).addClass('active');

          _this.$wrap.animate({'margin-left': -_this.$width*(idx-1)}, settings.speed, function(){
            _this.$page=idx;
            setSlide();
          });
      }
    }


    new slider({
      pause:6000,
      elmt:"mycarousel"
      },sliderSource);
     
      
    </script>
    
    <script>
    function hengshuping(){ 
          if(window.orientation==180||window.orientation==0){ 
                window.location.href=window.location.href;
           } 
        if(window.orientation==90||window.orientation==-90){ 
                window.location.href=window.location.href;       
            } 
    }
    
    if(window.addEventListener){
        window.addEventListener("onorientationchange" in window ? "orientationchange" : "resize", hengshuping, false);
    }else if(window.attachEvent){
        window.attachEvent("onorientationchange" in window ? "orientationchange" : "resize", hengshuping);
    }
        
    //window.addEventListener("onorientationchange" in window ? "orientationchange" : "resize", hengshuping, false);
    </script>
  </body>
</html>





